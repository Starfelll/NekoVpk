<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:NekoVpk.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:NekoVpk.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="NekoVpk.Views.MainWindow"
        Icon="/Assets/NekoVpk.ico"
        Title="NekoVpk 0.0.2"
		Width="1180" Height="768"
		WindowStartupLocation="CenterScreen"
		TransparencyLevelHint="Mica,Blur,None">
        <views:MainView />
		<Window.Background>
			<ImageBrush Source="/Assets/neko_box.png"
						AlignmentX="Right"
						AlignmentY="Bottom"
						TileMode="None"
						Stretch="None"
						Opacity="0.25"/>
		</Window.Background>
</Window>
